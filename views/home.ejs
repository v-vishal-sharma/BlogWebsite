<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach(function(p){%>
    <h1><%= p.title %></h1>
    <p>
        <%= p.content.substring(0,99) + " ..." %>
        <a href="/posts/<%= p.title %>">Read More</a>
    </p>
<%})%>

