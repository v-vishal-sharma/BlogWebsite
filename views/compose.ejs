<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <label for="newPost" class="form-label">Title</label>
    <input type="text" name="postTitle" class="form-content" autocomplete="off">
    <label for="postContent" class="form-label">Post</label>
    <input type="text" name="postContent" class="form-content" autocomplete="off">

    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer") -%>
